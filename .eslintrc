{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true
  },
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "class-methods-use-this": "off",
    "function-paren-newline": "off",
    "jsx-a11y/img-has-alt": "off",
    "object-curly-newline": "off",
    "react/jsx-space-before-closing": "off"
  }
}
