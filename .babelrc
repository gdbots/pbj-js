{
  "env": {
    "cjs": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ],
              "node": "current"
            },
            "modules": "commonjs",
            "useBuiltIns": true
          }
        ]
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "es6": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ],
              "node": "current"
            },
            "modules": false,
            "useBuiltIns": true
          }
        ]
      ],
      "plugins": [
        "lodash",
        "./build/use-lodash-es"
      ]
    }
  }
}
