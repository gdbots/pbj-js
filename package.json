{
  "name": "@gdbots/pbj",
  "version": "0.2.1",
  "description": "Pbj library for es6.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdbots/pbj-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run build:babel && npm run build:prepub",
    "build:babel": "BABEL_ENV=es6 babel src --out-dir ./dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./dist",
    "clean": "rimraf ./dist",
    "lint": "BABEL_ENV=cjs eslint src tests",
    "pub": "npm run build && cd dist && npm publish --access public",
    "test": "BABEL_ENV=cjs tape -r ./tests/babel-register.js './tests/**/*.test.js' | faucet"
  },
  "module": "./index.js",
  "jsnext:main": "./index.js",
  "dependencies": {
    "base-64": "^0.1.0",
    "bignumber.js": "^4.0.4"
  },
  "peerDependencies": {
    "@gdbots/common": "^0.1.0",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "utf8": "^2.1.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@gdbots/common": "^0.1.0",
    "aws-sdk-mock": "^1.7.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.26.0",
    "elasticsearch": "^13.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faucet": "^0.0.1",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "rimraf": "^2.6.2",
    "tape": "^4.6.3",
    "utf8": "^2.1.2",
    "uuid": "^3.1.0"
  }
}
