{
  "name": "@gdbots/pbj",
  "version": "0.1.0",
  "description": "Pbj library for es6.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdbots/pbj-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf lib",
    "test": "BABEL_ENV=cjs tape -r babel-register tests/**/*.test.js | faucet",
    "lint": "eslint src tests",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib/cjs",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir lib/es6",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build:cjs && npm run build:es6"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/es6/index.js",
  "jsnext:main": "./lib/es6/index.js",
  "dependencies": {
    "bignumber.js": "^4.0.2",
    "@gdbots/common": "git://github.com/gdbots/common-js.git#0.1.x-dev",
    "lodash": "^4.17.4",
    "mixin": "^0.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "aws-sdk-mock": "^1.7.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "elasticsearch": "^13.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faucet": "^0.0.1",
    "rimraf": "^2.6.1",
    "tape": "^4.6.3"
  }
}
