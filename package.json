    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "aws-sdk-mock": "^1.7.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "elasticsearch": "^13.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
